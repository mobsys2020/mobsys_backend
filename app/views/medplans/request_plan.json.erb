{
"patient": "<%=@user.name%>",
"doctor": "<%=@doctor.name%>",
"medcount": "<%=@meds.count%>",
"meds": [
	<% i = 1%>
	<% @meds.each do |med| %>
	{
		"name":"<%=med.name%>",
		"quantity": "<%=med.quantity%>",
		"time": "<%=med.time%>",
		"days": "<%=med.days%>"
    }<%if i < @meds.count %> , <%end%>
	<% i += 1%>
  <%end%>

]
}
