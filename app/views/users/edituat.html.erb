<% provide(:title, 'UAT') %>
<br>
<br>
<h1>User Access token ändern</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
  <form class="edit_user" id="edit_uat" action="/edit/useraccesstoken" accept-charset="UTF-8" method="post">
        <label for="user_uat">Uat</label>
      <input class="form-control" value="<%=@user.user_access_token%>" type="text" name="uat" id="user_access_token">
        <br>
        <br>
      <input type="submit" name="commit" value="UAT ändern" class="btn btn-primary" data-disable-with="UAT ändern">
      <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>" />
      </form>
  </div>
</div>